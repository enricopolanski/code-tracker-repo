{
  "name": "trackeo",
  "displayName": "trackeo",
  "publisher": "enricopolanski",
  "description": "Track time spent in vsc",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "title": "Trackeo",
      "properties": {
        "googlesheets.spreadsheetId": {
          "type": [
            "string",
            null
          ],
          "default": null,
          "description": "Google Sheets spreadsheet id. You can find if in the _url_ of your google spreadsheet in the web."
        },
        "googlesheets.worksheetTitle": {
          "type": [
            "string",
            null
          ],
          "default": null,
          "description": "Google Sheets worksheet title. You can find it at the _bottom_ of your google worksheet in the web."
        }
      }
    }
  },
  "markdown": "github",
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.79.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.3",
    "typescript": "~5.1.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1"
  },
  "dependencies": {
    "@effect/data": "~0.12.6",
    "@effect/io": "~0.26.0",
    "axios": "~1.4.0",
    "googleapis": "^118.0.0"
  }
}
